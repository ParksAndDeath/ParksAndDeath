
@{
    ViewData["Title"] = "DisplayBucketList";
}
@model List<UserParks>

<h1>Bucket List</h1>

<table>
    <tr>
        <th>Park Name</th>
        <th>City</th>
        <th>State</th>
        <th>Latitude</th>
        <th>Longitude</th>
        <th>Web Address</th>
        
    </tr>
    @foreach (UserParks parks in Model)
    {
        <tr>
            <td>@parks.ParkName</td>
            <td>@parks.City</td>
            <td>@parks.State</td>
            <td>@parks.Latitude</td>
            <td>@parks.Longitude</td>
            <td>@parks.Url</td>
           
            
            <td>
                <a href="../ParksDb/UpdateParkVisited?id=@parks.UsersParkIds">Cross Off Bucket List</a>
               
            </td>




        </tr>
    }
</table>
