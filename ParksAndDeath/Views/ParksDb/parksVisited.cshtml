
@{
    ViewData["Title"] = "parksVisited";
}

@model List<UserParks>
<style>
    body {
        background-image: url(https://free4kwallpapers.com/uploads/originals/2015/09/30/zion-desert-mountain-nature-hd-wallpaper.jpg);
    }
    table, th, td {
        border: 1px solid black;
        
    }

    th, td {
        padding: 8px;
    }

    .text-center {
        display: table;
        width: 100%;
        height: 100%;
        text-align: center;
    }

    .text-center-container {
        display: table-cell;
        width: 100%;
        vertical-align: middle;
    }

    .text-center-box {
        display: inline-block;
        padding: 50px;
        background-color: #ffffff;
    }
</style>


<h1 style="font-family:Cooper">Parks Visited</h1>


    <table class="table table-hover table-light">

        <tr>
            <th>Park Name</th>
            <th>Park Address</th>
            <th>Park State</th>
            <th>Latitude</th>
            <th>Longitude</th>
            <th>Web Address</th>
        </tr>
        @foreach (UserParks visited in Model)
        {
    <tr>
        <td>@visited.ParkName</td>
        <td>@visited.City</td>
        <td>@visited.State</td>
        <td>@visited.Latitude</td>
        <td>@visited.Longitude</td>
        <td><a href="@visited.Url">@visited.Url</a></td>
    </tr>
        }
    </table="">

